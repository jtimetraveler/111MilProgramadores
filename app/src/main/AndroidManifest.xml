<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="company.juancho.congresoFP">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:name="company.juancho.congresoFP.Principal"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_santafe"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_santafe"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="company.juancho.congresoFP.Main"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="company.juancho.congresoFP.Grilla"
            android:label="@string/title_activity_grilla"
            android:parentActivityName="company.juancho.congresoFP.Main">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="company.juancho.congresoFP.Main" />
        </activity>
        <activity
            android:name="company.juancho.congresoFP.Formulario"
            android:label="@string/title_activity_formulario"
            android:parentActivityName="company.juancho.congresoFP.Main"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="company.juancho.congresoFP.Main" />
        </activity>
        <activity
            android:name="company.juancho.congresoFP.Plano"
            android:label="@string/title_activity_plano"
            android:parentActivityName="company.juancho.congresoFP.Main"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="company.juancho.congresoFP.Main" />
        </activity>
        <activity
            android:name="company.juancho.congresoFP.Rosario"
            android:label="@string/title_activity_rosario"
            android:parentActivityName="company.juancho.congresoFP.Main"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="company.juancho.congresoFP.Main" />
        </activity>
    </application>

</manifest>